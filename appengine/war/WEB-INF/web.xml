<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>server</servlet-name>
        <servlet-class>org.dbwiki.appengine.DatabaseWikiAppEngineServlet</servlet-class>
    </servlet>    
    <servlet-mapping>
        <servlet-name>server</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server</servlet-name>
        <url-pattern>/drop</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server</servlet-name>
        <url-pattern>/adduser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server</servlet-name>
        <url-pattern>/dropuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>server</servlet-name>
        <url-pattern>/adddatabase</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>server.jsp</welcome-file>
    </welcome-file-list>
</web-app>
